{
  "name": "react-dependency-injection",
  "version": "0.0.1",
  "description": "Angular-style singleton services for React using Context API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "context",
    "services",
    "angular",
    "singleton",
    "dependency-injection"
  ],
  "author": "Nemanja Blagojevic",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "@types/react": "^18.0.0"
  },
  "peerDependencies": {
    "react": ">=17"
  }
}
